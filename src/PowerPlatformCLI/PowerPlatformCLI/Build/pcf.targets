<Project  xmlns='http://schemas.microsoft.com/developer/msbuild/2003' >

	<Target Name='PushPCFUpdate' DependsOnTargets="Build;PacSelectOrg" Condition="'$(IsPipeline)'!='true' And '$(SkipPCFPush)'!='true'" >


		<Exec Command="pac pcf push --publisher-prefix $(SolutionPublisherPrefix) --solution-unique-name $(SolutionUniqueName)"
			  WorkingDirectory="$(ProjectDir)"/>

		<!-- 
		SolutionUniqueName=$DataverseSolutionUniqueName);SolutionPublisherPrefix=$(DataverseSolutionCustomizationPrefix)"
		
		pac pcf push help -->

	</Target>

</Project>