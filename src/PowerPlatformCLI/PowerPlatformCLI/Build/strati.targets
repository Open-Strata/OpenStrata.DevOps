<Project  xmlns='http://schemas.microsoft.com/developer/msbuild/2003' >

	<PropertyGroup>
		<ShowHelpTargets>StratiHelp;$(ShowHelpTargets)</ShowHelpTargets>
	</PropertyGroup>

	<Target Name="StratiHelp" >

		<Message Text="**** Push Strati *************************************" Importance="high" />
		<Message Text="To push after build, commit and syn changes" Importance="high" />
		<Message Text="Alternatively, run the following command." Importance="high" />
		<Message Text="dotnet msbuild -p:PushAfterBuild=true" Importance="high" />


	</Target>


</Project>